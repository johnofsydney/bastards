<p style="color: green"><%= notice %></p>

<div class="container search-container">
  <div id="search">
    <%= render :partial => 'home/search_form' %>
    <p>Search for any Candidate, Electorate, Party, Prior Career or Education </p>
  </div>
</div>

<h1>Field of Study</h1>
<p>Incumbent MPs and Senators who are running for re-election.</p>

<div>
  <h2>General Population</h2>
  <%= pie_chart @genpop_field_of_studies, library: { pieSliceText: 'value' } %>
</div>
<div>
  <h2>ALP</h2>
  <%= pie_chart @alp_field_of_studies %>
</div>
<div>
  <h2>Liberals</h2>
  <%= pie_chart @lib_field_of_studies %>
</div>
<div>
  <h2>Nationals</h2>
  <%= pie_chart @nat_field_of_studies %>
</div>



<script src="https://code.jquery.com/jquery-3.5.1.js"></script>
<script src="https://cdn.datatables.net/1.11.5/js/jquery.dataTables.min.js"></script>

<script>
    $(document).ready(function () {
        $('#field-of-study-candidate-table').DataTable({
            paging: false,
            searching: false
        });
    });
</script>
